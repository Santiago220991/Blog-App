<p class="post_title">Add a new post</p>

<%= form_with model: @post, url: user_posts_path, class: "new_form", html:{"turbo-data": "false"}do  |f| %>
  <%= f.label :title, "Title" %>
  <%= f.text_field :title %>
  <%= f.label :text, "Text" %>
  <%= f.text_area :text %>
  <%= f.submit "Create Post"  %>
<% end %>